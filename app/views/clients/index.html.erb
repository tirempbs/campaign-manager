<h2>Clients</h2>

<div><%= link_to 'New Client', new_client_path %></div>

<ul>
  <% @clients.each do |c| %>
      <li><%= link_to c.name, client_path(c) %></li>
  <% end %>
</ul>

<h2>Campaigns</h2>

<div><%= link_to 'New Campaign', new_campaign_path %></div>

<ul>
  <% @campaigns.each do |c| %>
    <li><%= link_to c.name, campaign_path(c) %></li>
  <% end %>
</ul>